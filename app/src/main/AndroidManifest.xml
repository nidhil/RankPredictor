<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.nikhil.rankpredictor">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".RankPredictionActivity"
            android:label="Predict your Rank"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".MockUpActivity"
            android:parentActivityName=".MainActivity"
            android:label="Mock Test 1"
            />
        <activity
            android:name=".ShowRankActivity"
            android:label="Predicted Rank"
            android:parentActivityName=".RankPredictionActivity" />
        <activity android:name=".ShowResultActivity"
            android:parentActivityName=".MockUpActivity"
            android:label="Results" />
    </application>

</manifest>